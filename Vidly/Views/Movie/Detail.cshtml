@model Vidly.Models.Movie
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.strMovieName</h2>

<ul>
    <li>Genre: <b> @Model.Genre.Name </b></li>
    <li>Release Date: @Model.ReleaseDate</li>
    <li>Date Added : @Model.DateAdded</li>
    <li>Number In Stock : @Model.NumberInStock</li>
    <li>Available Stock : @Model.NumberAvailable</li>
</ul>

